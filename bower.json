{
  "name": "etherisc-dashboard",
  "description": "Etherisc Flight Policies Dashboard",
  "license": "MIT",
  "homepage": "",
  "ignore": [
    "**/.*",
    "node_modules",
    "test",
    "tests"
  ],
  "copy": [
    {
      "src": [
        "angular/angular.js",
        "angular-animate/angular-animate.js",
        "angular-resource/angular-resource.js",
        "angular-sanitize/angular-sanitize.js",
        "angular-touch/angular-touch.js",
        "angular-strap/dist/angular-strap.js",
        "angular-strap/dist/angular-strap.tpl.js",
        "angular-bootstrap/ui-bootstrap-tpls.min.js",
        "angular-bootstrap-nav-tree/dist/**",
        "angular-file-upload/angular-file-upload.js",
        "angular-ui-grid/ui-grid.*",
        "angular-ui-map/ui-map.js",
        "angular-ui-router/release/**",
        "angular-ui-select/dist/**",
        "angular-ui-utils/ui-utils.js",
        "jquery.easy-pie-chart/dist/angular.easypiechart.js",
        "ngImgCrop/compile/minified/**",
        "ngstorage/ngStorage.js",
        "oclazyload/dist/**",
        "venturocket-angular-slider/build/**",
        "angular-echarts/dist/**",
        "html.sortable/dist/html.sortable.angular.min.js"
      ],
      "dest": "libs/angular",
      "cwd": "bower_components",
      "expand": true
    },
    {
      "src": [
        "echarts/build/dist/**",
        "moment/moment.js",
        "moment/locale/**",
        "underscore/underscore-min.js",
        "dropzone/dist/min/**"
      ],
      "dest": "libs/js",
      "cwd": "bower_components",
      "expand": true
    },
    {
      "src": [
        "jquery/dist/jquery.js",
        "bootstrap/dist/js/bootstrap.js",
        "tether/dist/js/tether.min.js",
        "PACE/pace.min.js",
        "jQuery-Storage-API/jquery.storageapi.min.js",
        "datatables/media/js/jquery.dataTables.min.js",
        "plugins/integration/bootstrap/3/**",
        "plugins/integration/bootstrap/images/**",
        "footable/dist/footable.all.min.js",
        "footable/css/footable.core.css",
        "footable/css/fonts/**",
        "bower-jvectormap/*.js",
        "flot/jquery.flot.js",
        "flot/jquery.flot.resize.js",
        "flot/jquery.flot.pie.js",
        "flot.tooltip/js/jquery.flot.tooltip.min.js",
        "flot-spline/js/jquery.flot.spline.min.js",
        "flot.orderbars/js/jquery.flot.orderBars.js",
        "waves/dist/**",
        "eonasdan-bootstrap-datetimepicker/build/**",
        "select2/dist/**",
        "select2-bootstrap-theme/dist/**",
        "parsleyjs/dist/**",
        "html.sortable/dist/html.sortable.min.js",
        "nestable/jquery.nestable.js",
        "jquery-pjax/jquery.pjax.js",
        "twitter-bootstrap-wizard/jquery.bootstrap.wizard.js"
      ],
      "dest": "libs/jquery",
      "cwd": "bower_components",
      "expand": true
    },
    {
      "src": [
        "animate.css/animate.min.css",
        "font-awesome/css/**",
        "font-awesome/fonts/**",
        "material-design-icons/iconfont/**",
        "angular-motion/dist/*.*",
        "bootstrap-additions/dist/*.*",
        "bootstrap/dist/css/**",
        "bootstrap/scss/**"
      ],
      "dest": "assets",
      "cwd": "bower_components",
      "expand": true
    }
  ],
  "dependencies": {
    "PACE": "pace#~1.0.2",
    "angular": "1.5.3",
    "angular-animate": "1.5.3",
    "angular-bootstrap": "~0.14.3",
    "angular-resource": "1.5.3",
    "angular-sanitize": "1.5.3",
    "angular-strap": "~2.3.2",
    "angular-touch": "1.5.3",
    "angular-ui-router": "~0.2.11",
    "angular-ui-utils": "~0.2.1",
    "bootstrap": "4.0.0-alpha.4",
    "bower-jvectormap": "~1.2.2",
    "echarts": "~2.2.7",
    "flot": "~0.8.3",
    "flot-spline": "*",
    "flot.orderbars": "*",
    "flot.tooltip": "~0.8.4",
    "jquery": "2.1.3",
    "jquery.easy-pie-chart": "~2.1.6",
    "jquery.sparkline": "~2.1.2",
    "moment": "2.9.0",
    "ngstorage": "~0.3.0",
    "oclazyload": "1.0.9",
    "tether": "~1.1.0",
    "font-awesome": "^4.7.0"
  }
}
